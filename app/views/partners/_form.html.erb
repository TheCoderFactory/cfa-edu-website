<div class="container">
  <%= simple_form_for(@partner) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :url %>
      <%= f.input :name %>
      <%= f.input :image, label: 'image size: 300px * 100px'%>
      <%= f.input :order %>
      <%= f.input :active %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
</div>
